{
    "name": "connectholland/tulip-api-bundle",
    "description": "Tulip API integration for Symfony Framework version 2.8 and 3.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Nijens",
            "email": "niels@connectholland.nl"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "connectholland/tulip-api-client": "^1.0",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ConnectHolland\\TulipAPIBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConnectHolland\\TulipAPIBundle\\Tests": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
